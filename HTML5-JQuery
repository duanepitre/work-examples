<!--- jQuery / Video thumbnail states --->
<script>
	$(document).ready(function() {
		var initial = $('.initial-video')
		initial.find('img').addClass('video-thumb-active');
		initial.find('p').addClass('video-caption-active');
		$('.videos li').on('click', function() {
			$('.video-thumb').removeClass('video-thumb-active');
			$(this).find('.video-thumb').addClass('video-thumb-active');
			$('.video-caption').removeClass('video-caption-active')
			$(this).find('.video-caption').addClass('video-caption-active');
		});
	});
</script>
